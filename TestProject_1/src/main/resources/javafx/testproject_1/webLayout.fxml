<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox maxHeight="1080.0" maxWidth="1920.0" minHeight="1080.0" minWidth="1920.0" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox maxHeight="1080.0" maxWidth="1080.0" minHeight="1080.0" minWidth="1080.0" prefHeight="1080.0" prefWidth="1080.0" style="-fx-background-color: #381819;" />
      <VBox maxHeight="1080.0" maxWidth="840.0" minHeight="1080.0" minWidth="840.0" prefHeight="1080.0" prefWidth="840.0" style="-fx-background-color: yellow;">
         <children>

             <HBox maxHeight="50.0" maxWidth="840.0" minHeight="50.0" minWidth="840.0" prefHeight="50.0" prefWidth="840.0" spacing="5" style="-fx-background-color: lightpink;">
                 <children>
                     <TextField maxHeight="45.0" maxWidth="415.0" minHeight="45.0" minWidth="415.0" prefHeight="45.0" prefWidth="415.0" promptText="Enter Something..." style="-fx-background-radius: 15;">
                     <HBox.margin>
                        <Insets bottom="2.5" left="5.0" top="2.5" />
                     </HBox.margin>
                     <font>
                        <Font size="20.0" />
                     </font></TextField>
                  <Button maxHeight="45.0" maxWidth="200.0" minHeight="45.0" minWidth="200.0" prefHeight="45.0" prefWidth="200.0" style="-fx-background-radius: 30;" text="OPEN" textAlignment="CENTER">
                     <font>
                        <Font name="Arial Black" size="16.0" />
                     </font>

                  </Button>
                  <Button maxHeight="45.0" maxWidth="200.0" minHeight="45.0" minWidth="200.0" prefHeight="45.0" prefWidth="200.0" style="-fx-background-radius: 30;" text="ADD" textAlignment="CENTER">
                     <font>
                        <Font name="Arial Black" size="16.0" />
                     </font>

                  </Button>
                 </children>
             </HBox>

             <HBox maxHeight="250.0" maxWidth="840.0" minHeight="250.0" minWidth="840.0" prefHeight="250.0" prefWidth="840.0" style="-fx-background-color: #e0e0e0;">
                 <children>
                     <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="840.0">
                         <content>
                             <TableView maxHeight="250.0" maxWidth="840.0" minHeight="250.0" minWidth="840.0" prefHeight="250.0" prefWidth="840.0">
                                 <columns>
                                     <TableColumn prefWidth="210.0" text="Column 1" />
                                     <TableColumn prefWidth="210.0" text="Column 2" />
                                     <TableColumn prefWidth="210.0" text="Column 3" />
                                     <TableColumn prefWidth="210.0" text="Column 4" />
                                 </columns>
                             </TableView>
                         </content>
                     </ScrollPane>
                 </children>
             </HBox>

             <HBox maxHeight="400.0" maxWidth="840.0" minHeight="400.0" minWidth="840.0" prefHeight="400.0" prefWidth="840.0" spacing="0" style="-fx-background-color: green;">
                 <children>
                     <!-- First VBox with TilePane -->
                     <VBox prefHeight="400.0" prefWidth="420.0" style="-fx-background-color: lightgrey;">
                         <children>
                             <HBox spacing="0">
                                 <children>
                                     <VBox spacing="2">
                                         <children>
                                             <Button maxHeight="94.0" maxWidth="204.0" minHeight="94.0" minWidth="204.0" prefHeight="94.0" prefWidth="204.0" style="-fx-margin: 2; -fx-background-radius: 30;" text="ZOOM IN">
                                       <font>
                                          <Font name="Calibri Bold" size="20.0" />
                                       </font></Button>
                                    <Button maxHeight="94.0" maxWidth="204.0" minHeight="94.0" minWidth="204.0" prefHeight="94.0" prefWidth="204.0" style="-fx-margin: 2; -fx-background-radius: 30;" text="REDO">
                                       <font>
                                          <Font name="Calibri Bold" size="20.0" />
                                       </font>
                                    </Button>
                                    <Button maxHeight="94.0" maxWidth="204.0" minHeight="94.0" minWidth="204.0" prefHeight="94.0" prefWidth="204.0" style="-fx-margin: 2; -fx-background-radius: 30;" text="DELETE">
                                       <font>
                                          <Font name="Calibri Bold" size="20.0" />
                                       </font>
                                    </Button>
                                    <Button maxHeight="94.0" maxWidth="204.0" minHeight="94.0" minWidth="204.0" prefHeight="94.0" prefWidth="204.0" style="-fx-margin: 2; -fx-background-radius: 30;" text="FIT">
                                       <font>
                                          <Font name="Calibri Bold" size="20.0" />
                                       </font>
                                    </Button>
                                         </children>
                                     </VBox>
                                     <VBox spacing="2">
                                         <children>
                                             <Button maxHeight="94.0" maxWidth="204.0" minHeight="94.0" minWidth="204.0" prefHeight="94.0" prefWidth="204.0" style="-fx-margin: 2; -fx-background-radius: 30;" text="ZOOM OUT">
                                       <font>
                                          <Font name="Calibri Bold" size="20.0" />
                                       </font>
                                       <VBox.margin>
                                          <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                                       </VBox.margin></Button>
                                    <Button maxHeight="94.0" maxWidth="204.0" minHeight="94.0" minWidth="204.0" prefHeight="94.0" prefWidth="204.0" style="-fx-margin: 2; -fx-background-radius: 30;" text="UNDO">
                                       <font>
                                          <Font name="Calibri Bold" size="20.0" />
                                       </font>
                                    </Button>
                                    <Button maxHeight="94.0" maxWidth="204.0" minHeight="94.0" minWidth="204.0" prefHeight="94.0" prefWidth="204.0" style="-fx-margin: 2; -fx-background-radius: 30;" text="SAVE">
                                       <font>
                                          <Font name="Calibri Bold" size="20.0" />
                                       </font>
                                    </Button>
                                    <Button maxHeight="94.0" maxWidth="204.0" minHeight="94.0" minWidth="204.0" prefHeight="94.0" prefWidth="204.0" style="-fx-margin: 2; -fx-background-radius: 30;" text="RESET">
                                       <font>
                                          <Font name="Calibri Bold" size="20.0" />
                                       </font>
                                    </Button>
                                         </children>
                                     </VBox>
                                 </children>
                             </HBox>
                         </children>
                     </VBox>
                     <!-- Second VBox (empty or you can fill later) -->
                     <VBox maxHeight="400.0" maxWidth="420.0" minHeight="400.0" minWidth="420.0" prefHeight="400.0" prefWidth="420.0" style="-fx-background-color: #FAFAD2;">
                         <!-- Add content here later -->
                     </VBox>
                 </children>
             </HBox>


             <HBox maxHeight="300.0" maxWidth="840.0" minHeight="300.0" minWidth="840.0" prefHeight="300.0" prefWidth="840.0" style="-fx-background-color: #98FB98;" />
            <HBox maxHeight="80.0" maxWidth="840.0" minHeight="80.0" minWidth="840.0" prefHeight="80.0" prefWidth="840.0" style="-fx-background-color: #FFECB3;" />
         </children>
      </VBox>
   </children>
</HBox>
